        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">Any information that is reported from a process. [MMI OntDev]</skos:definition>
    </owl:Class>
    


