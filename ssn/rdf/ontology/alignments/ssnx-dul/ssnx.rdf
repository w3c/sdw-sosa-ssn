        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">A sensing device is a device that implements sensing.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Sensitivity -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Sensitivity">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/ssn/systems/Sensitivity"/>
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementProperty"/>
        <dcterms:source>skos:exactMatch &apos;sensitivity&apos; [VIM 4.12]
      http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf</dcterms:source>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
        <rdfs:label xml:lang="en">Sensitivity</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">Sensitivity is the quotient of the change in a result of sensor and the corresponding change in a value of a quality being observed.</skos:definition>
