        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/systems/Resolution"/>
        <dcterms:source>skos:exactMatch &apos;resolution&apos; [VIM 4.14]
      http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf</dcterms:source>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
        <rdfs:label xml:lang="en">Resolution</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">The smallest difference in the value of a quality being observed that would result in perceptably different values of observation results.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#ResponseTime -->

