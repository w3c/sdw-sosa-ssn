        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">An Entity to which other Entities can be attached - particuarly Sensors and other Platforms.  For example, a post might act as a Platform, a buoy might act as a Platform, or a fish might act as a Platform for an attached sensor.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Precision -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Precision">
