        <dcterms:source>skos:exactMatch &apos;property&apos; [O&amp;M]
             http://www.opengeospatial.org/standards/om</dcterms:source>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
        <rdfs:label xml:lang="en">Property</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <skos:definition xml:lang="en">An observable Quality of an Event or Object.  That is, not a Quality of an abstract entity, but rather an aspect of an entity that is intrinsic to and cannot exist without the entity and that is observable by a sensor.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#Resolution -->

