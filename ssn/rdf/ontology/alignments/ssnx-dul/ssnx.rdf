        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/systems/Selectivity"/>
        <dcterms:source>skos:exactMatch &apos;selectivity&apos; [VIM 4.13]
      http://www.bipm.org/utils/common/documents/jcgm/JCGM_200_2008.pdf</dcterms:source>
