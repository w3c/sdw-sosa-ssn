@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <http://www.w3.org/ns/ssn/systems/> .

<TemperatureSensor/Datasheet> a sosa:Procedure, ssn:System ;
    rdfs:comment "This is a datasheet giving the specifications of a particular sensor kind"@en ;
    ssn-system:hasOperatingRange ... ;
    ssn-system:hasSystemCapability ... .

<TemperatureSensor/Config1> a sosa:Procedure ;
    rdfs:comment "This is a particular configuration/procedure used for this sensor"@en .

<TemperatureSensor/Instance1> a sosa:Sensor ;
    rdfs:comment "This is a sensor instance"@en ;
    ssn:implements <TemperatureSensor/Datasheet> .

<Obs/1087> a sosa:Observation ;
    rdfs:comment "An observation refers to both the sensor and the specific configuration/procedure used for this measurement"@en ;
    sosa:madeBySensor <TemperatureSensor/Instance1> ;
    sosa:usedProcedure <TemperatureSensor/Config1> ;
    sosa:resultTime "2024-01-18T20:00:00Z" ;
    sosa:hasSimpleResult "15.2 Cel" .
